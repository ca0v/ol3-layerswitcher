{
  "name": "ol3-layerswitcher",
  "version": "5.1.3",
  "description": "Grouped layer list control for an OpenLayers map.",
  "main": "index.ts",
  "scripts": {
    "clean": "rmdir .\\built /s/q & del test\\*.js /s",
    "uglify": "pushd built && uglifyjs index.max.js --source-map \"content='index.max.js.map',url='index.js.map'\" --output=index.js --compress --mangle && popd",
    "build-for-rawgit": "tsc -p tsconfig-examples.json",
    "live": "live-server --port=5000 --wait=2000 --ignorePattern=.*\\.ts --entry-file=rawgit.html?debug=1",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ca0v/ol3-layerswitcher.git"
  },
  "keywords": [
    "ol"
  ],
  "author": "coreyalix@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ca0v/ol3-layerswitcher/issues"
  },
  "homepage": "https://github.com/ca0v/ol3-layerswitcher#readme",
  "devDependencies": {
    "@types/jquery": "^3.3.6",
    "@types/ol": "^4.6.2",
    "@types/requirejs": "^2.1.31",
    "karma": "^3.0.0",
    "live-server": "^1.2.0",
    "typescript": "^3.0.1",
    "uglify-js": "^3.4.8"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "ol": "^5.1.3",
    "ol3-fun": "https://github.com/ca0v/ol3-fun/tarball/v5.1.3",
    "proj4": "^2.4.4",
    "requirejs": "^2.3.5"
  }
}
